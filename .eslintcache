 = { ...obj, [key]: props.currentDevice[key] };\n      }\n    }\n    return obj;\n  }\n  const objUrlsImages = getObjUrlsImages();\n\n  function onChange(e) {\n    const nextDevice = reallyTheSameDevices.find(\n      (item) =>\n        parseInt(item.memory, 10) === e.target.value &&\n        item.color === props.currentDevice.color\n    );\n    const nextUrl = props.location.pathname.replace(\n      props.match.params.urlName,\n      nextDevice.url\n    );\n\n    props.history.push(nextUrl);\n  }\n\n  function handleChangeColor(e) {\n    const nextDevice = reallyTheSameDevices.find(\n      (item) =>\n        item.color === e.target.value &&\n        item.memory === props.theReduxStore.currentDevice.memory\n    );\n    const nextUrl = props.location.pathname.replace(\n      props.match.params.urlName,\n      nextDevice.url\n    );\n    const currScrollY = window.scrollY;\n    console.log(currScrollY);\n    props.history.push(nextUrl);\n    window.scrollTo(0, currScrollY);\n  }\n\n  function handleAddToCart(item) {\n    if (!props.cart.find((i) => i.id === item.id)) {\n      props.dispatch(addItemToCart({ ...item, quantity: 1, cost: item.price }));\n    }\n  }\n\n  return (\n    <>\n      <div className=\"cont-imgcarousel-and-featuresblock\">\n        <ImageCarouselDevice\n          objUrlsImages={objUrlsImages}\n          arrUrlsImages={urlsImages}\n        />\n        <div className=\"features-block\">\n          <h1 className=\"features-block-h1\">\n            {props.currentDevice.title +\n              \" \" +\n              props.currentDevice.memory +\n              \" \" +\n              props.currentDevice.color}\n          </h1>\n\n          <Button\n            onClick={() => handleAddToCart(props.currentDevice)}\n            type=\"primary\"\n            size=\"large\"\n            style={{ marginBottom: \"5%\" }}\n          >\n            Add to Cart\n          </Button>\n\n          <SelectMemory\n            onChange={onChange}\n            currentMemory={parseInt(props.currentDevice.memory)}\n            availableMemory={arrOfAvailableMemoryDevice}\n          />\n\n          <SelectColor\n            availableColors={availableColors}\n            currentDeviceColor={props.currentDevice.color}\n            onChangeColor={handleChangeColor}\n          />\n\n          <CharacteristicsTable currentDevice={props.currentDevice} />\n          <a href=\"#info-block\">See more...</a>\n        </div>\n      </div>\n      <DeviceInfoBlock />\n    </>\n  );\n}\n\nfunction mapStateToProps(state, ownProps) {\n  return {\n    theReduxStore: state,\n    currentDevice: state.currentDevice,\n    cart: state.cart,\n  };\n}\n\n// const mapDispatchToProps = {\n//   addItemToCart,\n// };\n\nexport default connect(mapStateToProps)(DevicePage);\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/HonorPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/SamsungPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/HuaweiPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/ApplePage.js",["243","244","245","246"],"import React, { useEffect } from \"react\";\nimport { Row } from \"antd\";\nimport CardDevice from \"../common/CardDevice\";\nimport { loadApple } from \"../../redux/actions/appleActions\";\nimport { loadCart, addItemToCart } from \"../../redux/actions/cartActions\";\nimport { connect } from \"react-redux\";\nimport { handleError } from \"../../api/apiUtils\";\nimport { withRouter } from \"react-router\";\n\nfunction ApplePage(props) {\n  // console.log(props);\n  // useEffect(() => {\n  //   props.loadApple().catch((error) => console.log(error));\n  // }, []);\n\n  // function handleAddToCart(event) {\n  //   event.preventDefault();\n  //   const foundIphone = props.apple.find(\n  //     (iphone) => iphone.id === parseInt(event.target.id)\n  //   );\n  //   if (!props.cart.find((item) => item.id === foundIphone.id)) {\n  //     props.addItemToCart(foundIphone);\n  //   }\n  // }\n\n  function handleAddToCart(item) {\n    if (!props.cart.find((i) => i.id === item.id)) {\n      props.addItemToCart({ ...item, quantity: 1, cost: item.price });\n    }\n  }\n\n  const arrOfAllAppleGoods = Object.values(props.apple).flat();\n\n  function checkUrl() {\n    switch (props.location.pathname) {\n      case \"/apple\":\n        return arrOfAllAppleGoods;\n      case \"/apple/iphones\":\n        return props.apple.iphones;\n      case \"/apple/ipads\":\n        return props.apple.ipads;\n      case \"/apple/macbooks\":\n        return props.apple.macbooks;\n      default:\n        return undefined;\n    }\n  }\n\n  return (\n    <>\n      <Row>\n        <CardDevice list={checkUrl()} onAddToCart={handleAddToCart} />\n\n        {/* {props.location.pathname === \"/apple/iphones\" && (\n          <CardDevice\n            list={props.apple.iphones}\n            onAddToCart={handleAddToCart}\n          />\n        )}\n\n        {props.location.pathname === \"/apple/ipads\" && (\n          <CardDevice list={props.apple.ipads} onAddToCart={handleAddToCart} />\n        )}\n\n        {props.location.pathname === \"/apple/macbooks\" && (\n          <CardDevice\n            list={props.apple.macbooks}\n            onAddToCart={handleAddToCart}\n          />\n        )} */}\n\n        {/* {arrOfAllAppleGoods.map((item) => (\n          <CardDevice\n            list={arrOfAllAppleGoods}\n            key={item.id}\n            onAddToCart={() => handleAddToCart(item)}\n            id={item.id}\n            url={item.url}\n            title={item.title}\n            path={item.path}\n            // description={item.description}\n            price={\"$\" + item.price}\n          />\n        ))} */}\n      </Row>\n    </>\n  );\n}\n\nfunction mapStateToProps(state, ownProps) {\n  return {\n    apple: state.apple,\n    cart: state.cart,\n  };\n}\n\nconst mapDispatchToProps = {\n  //   loadCart,\n  addItemToCart,\n  //   loadApple,\n};\n\nexport default connect(\n  mapStateToProps,\n  mapDispatchToProps\n)(withRouter(ApplePage));\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/XiaomiPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/cart/CartPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/cart/CheckoutPage.js",["247","248","249","250","251","252"],"import React, { useEffect } from \"react\";\r\nimport { connect } from \"react-redux\";\r\nimport { useState } from \"react\";\r\nimport { Form, Input, Button, Select, Modal } from \"antd\";\r\nimport { statesOfAmerica } from \"../../USAstates\";\r\nimport { cleanCart } from \"../../redux/actions/cartActions\";\r\nimport { addOrderApi } from \"../../api/ordersApi\";\r\nimport FormCheckout from \"./FormCheckout\";\r\n\r\n// const validateMessages = {\r\n//   required: \"${label} is required!\",\r\n// };\r\n\r\nconst CheckoutPage = (props) => {\r\n  const [americanState, setAmericanState] = useState(\"Ohio\");\r\n\r\n  function onFinish(values) {\r\n    console.log(\"Received values of form: \", values);\r\n    addOrderApi({\r\n      ...values,\r\n      subtotal: subtotal,\r\n      stateTax: stateTax,\r\n      total: total,\r\n      order: props.cart,\r\n    });\r\n    props.cleanCart(props.cart);\r\n    success();\r\n  }\r\n\r\n  function success() {\r\n    Modal.success({\r\n      content:\r\n        \"Your Order has been received! Our Manager will contact you soon to clarify the details and confirm the order.\",\r\n      maskClosable: true,\r\n    });\r\n  }\r\n\r\n  const subtotal = props.cart.reduce(\r\n    (sum, current) => sum + parseInt(current.cost),\r\n    0\r\n  );\r\n  const stateTax = (subtotal * (0.01 * americanState.length)).toFixed(2);\r\n\r\n  const total = (subtotal + parseFloat(stateTax) + 17).toFixed(2);\r\n\r\n  return (\r\n    <>\r\n      {subtotal === 0 ? (\r\n        <h5\r\n          style={{\r\n            backgroundColor: \"white\",\r\n            height: \"100px\",\r\n            paddingTop: \"1%\",\r\n            marginRight: \"70%\",\r\n          }}\r\n        >\r\n          Your cart is currently empty.\r\n        </h5>\r\n      ) : (\r\n        <div\r\n          style={{\r\n            display: \"flex\",\r\n            justifyContent: \"space-around\",\r\n            margin: \"2% 10% 0 10%\",\r\n          }}\r\n        >\r\n          <FormCheckout\r\n            onFinish={onFinish}\r\n            setAmericanStateProp={(value) => setAmericanState(value)}\r\n          ></FormCheckout>\r\n\r\n          <div\r\n            style={{\r\n              width: \"40%\",\r\n              marginLeft: \"30px\",\r\n              backgroundColor: \"white\",\r\n              padding: \"10px\",\r\n            }}\r\n          >\r\n            <h4>Your order</h4>\r\n            <table\r\n              width=\"100%\"\r\n              rules=\"rows\"\r\n              style={{\r\n                width: \"100%\",\r\n                height: \"330px\",\r\n                borderBottom: \"1px solid\",\r\n              }}\r\n            >\r\n              <thead>\r\n                <tr>\r\n                  <th>Product</th>\r\n                  <th>Subtotal</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody style={{ fontSize: \"1.2em\" }}>\r\n                {props.cart.map((item) => (\r\n                  <tr key={item.id}>\r\n                    <td>\r\n                      {item.title} x {item.quantity}\r\n                    </td>\r\n                    <td>${item.cost}</td>\r\n                  </tr>\r\n                ))}\r\n              </tbody>\r\n              <tfoot style={{ fontSize: \"1.3em\", fontWeight: \"bold