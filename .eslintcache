\"redux-immutable-state-invariant\";\nimport thunk from \"redux-thunk\";\n\nconst persistConfig = {\n  key: \"root\",\n  storage,\n  whitelist: [\"cart\"],\n};\n\nconst persistedReducer = persistReducer(persistConfig, rootReducer);\n\nexport default function configureStore(initialState) {\n  const composeEnhanser =\n    window.__REDUX_DEVTOOLS_EXTENSION_COMPOSE__ || compose;\n  return createStore(\n    persistedReducer,\n    initialState,\n    composeEnhanser(applyMiddleware(thunk, reduxImmutableStateInvariant()))\n  );\n}\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/route.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/common/Footer.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/common/Sidebar.js",["228","229"],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/common/Header.js",["230"],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/redux/reducers/index.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/HomePage.js",["231"],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/common/NotFoundPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/common/DevicePage.js",["232","233","234","235"],"import React from \"react\";\nimport { useState, useEffect } from \"react\";\nimport PropTypes from \"prop-types\";\nimport { useLocation } from \"react-router-dom\";\nimport { connect } from \"react-redux\";\nimport SelectColor from \"./device_page/SelectColor\";\nimport SelectMemory from \"./device_page/SelectMemory\";\nimport ImageCarouselDevice from \"./device_page/ImageCarouselDevice\";\nimport CharacteristicsTable from \"./device_page/CharacteristicsTable\";\nimport { Button } from \"antd\";\nimport { addItemToCart } from \"../../redux/actions/cartActions\";\nimport { Anchor } from \"antd\";\nimport DeviceInfoBlock from \"./device_page/DeviceInfoBlock\";\n\nimport {\n  addCurrentDevice,\n  deleteCurrentDevice,\n} from \"../../redux/actions/devicePageActions\";\n\nfunction DevicePage(props) {\n  function getArrSameTypesOfDevices() {\n    const arrCurrentDevice = props.location.pathname.split(\"/\");\n    const arrTheSameTypesOfDevices =\n      props.theReduxStore[arrCurrentDevice[1]][arrCurrentDevice[2]];\n    return arrTheSameTypesOfDevices;\n  }\n  const theSameTypesOfDevices = getArrSameTypesOfDevices();\n\n  function getCurrentDevice() {\n    // const arrCurrentDevice = props.location.pathname.split(\"/\");\n    // const arrTheSameDevices =\n    //   props.theReduxStore[arrCurrentDevice[1]][arrCurrentDevice[2]];\n    const objCurrentDevice = theSameTypesOfDevices.find(\n      (item) => item.url === props.match.params.urlName\n    );\n    return objCurrentDevice;\n  }\n  const _currentDevice = getCurrentDevice();\n\n  const reallyTheSameDevices = theSameTypesOfDevices.filter(\n    (item) => item.title === props.theReduxStore.currentDevice.title\n  );\n\n  const availableColors = [\n    ...new Set(reallyTheSameDevices.map((i) => i.color)),\n  ];\n\n  const arrOfAvailableMemoryDevice = [\n    ...new Set(reallyTheSameDevices.map((i) => i.memory)),\n  ];\n\n  useEffect(\n    function () {\n      if (_currentDevice) props.dispatch(addCurrentDevice(_currentDevice));\n      return () => props.dispatch(deleteCurrentDevice(_currentDevice));\n    },\n    [_currentDevice]\n  );\n\n  function getUrlsImages() {\n    const arr = [];\n    for (const key in props.currentDevice) {\n      if (key.startsWith(\"image\")) {\n        arr.push(props.currentDevice[key]);\n      }\n    }\n    return arr;\n  }\n  const urlsImages = getUrlsImages();\n\n  function getObjUrlsImages() {\n    let obj = {};\n    for (const key in props.currentDevice) {\n      if (key.startsWith(\"image\")) {\n        obj = { ...obj, [key]: props.currentDevice[key] };\n      }\n    }\n    return obj;\n  }\n  const objUrlsImages = getObjUrlsImages();\n\n  function handleChangeMemory(e) {\n    const nextDevice = reallyTheSameDevices.find(\n      (item) =>\n        parseInt(item.memory, 10) === e.target.value &&\n        item.color === props.currentDevice.color\n    );\n    const nextUrl = props.location.pathname.replace(\n      props.match.params.urlName,\n      nextDevice.url\n    );\n\n    props.history.push(nextUrl);\n  }\n\n  function handleChangeColor(e) {\n    const nextDevice = reallyTheSameDevices.find(\n      (item) =>\n        item.color === e.target.value &&\n        item.memory === props.theReduxStore.currentDevice.memory\n    );\n    const nextUrl = props.location.pathname.replace(\n      props.match.params.urlName,\n      nextDevice.url\n    );\n    props.history.push(nextUrl);\n  }\n\n  function handleAddToCart(item) {\n    if (!props.cart.find((i) => i.id === item.id)) {\n      props.dispatch(addItemToCart({ ...item, quantity: 1, cost: item.price }));\n    }\n  }\n\n  return (\n    <>\n      <div className=\"cont-imgcarousel-and-featuresblock\">\n        <ImageCarouselDevice\n          objUrlsImages={objUrlsImages}\n          arrUrlsImages={urlsImages}\n        />\n        <div className=\"features-block\">\n          <h1 className=\"features-block-h1\">\n            {`${props.currentDevice.fullTitle}`}\n          </h1>\n\n          <Button\n            className=\"features-block-btn-addtocart\"\n            onClick={() => handleAddToCart(props.currentDevice)}\n            type=\"primary\"\n            size=\"large\"\n          >\n            Add to Cart\n          </Button>\n\n          <SelectMemory\n            onChangeMemory={handleChangeMemory}\n            currentMemory={parseInt(props.currentDevice.memory)}\n            availableMemory={arrOfAvailableMemoryDevice}\n          />\n\n          <SelectColor\n            onChangeColor={handleChangeColor}\n            currentDeviceColor={props.currentDevice.color}\n            availableColors={availableColors}\n          />\n\n          <CharacteristicsTable currentDevice={props.currentDevice} />\n          <a className=\"features-block-anchor-to-descr\" href=\"#info-block\">\n            See more...\n          </a>\n        </div>\n      </div>\n      <DeviceInfoBlock />\n    </>\n  );\n}\n\nDevicePage.propTypes = {\n  theReduxStore: PropTypes.object.isRequired,\n  currentDevice: PropTypes.object.isRequired,\n  cart: PropTypes.array.isRequired,\n};\n\nfunction mapStateToProps(state, ownProps) {\n  return {\n    theReduxStore: state,\n    currentDevice: state.currentDevice,\n    cart: state.cart,\n  };\n}\n\n// const mapDispatchToProps = {\n//   addItemToCart,\n// };\n\nexport default connect(mapStateToProps)(DevicePage);\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/HonorPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/SamsungPage.js",["236"],"import React from \"react\";\nimport PropTypes from \"prop-types\";\nimport { Row } from \"antd\";\nimport ListOfCardsDevices from \"../common/ListOfCardsDevices\";\nimport { connect } from \"react-redux\";\nimport { addItemToCart } from \"../../redux/actions/cartActions\";\n\nfunction SamsungPage(props) {\n  const handleAddToCart = (item) => {\n    if (!props.cart.find((i) => i.id === item.id)) {\n      props.addItemToCart({ ...item, quantity: 1, cost: item.price });\n    }\n  };\n\n  function addBrandToObj(arrOfDevices) {\n    return { ...arrOfDevices, brand: \"samsung\" };\n  }\n\n  const arrOfAllSamsungGoods = Object.values(props.samsung)\n    .flat()\n    .map((d) => addBrandToObj(d));\n\n  return (\n    <div>\n      <h2>Samsung</h2>\n      <Row>\n        <section className=\"list-devices-main-cont\">\n          <ListOfCardsDevices\n            list={arrOfAllSamsungGoods}\n            // onAddToCart={handleAddToCart}\n          />\n        </section>\n      </Row>\n    </div>\n  );\n}\n\nSamsungPage.propTypes = {\n  samsung: PropTypes.object.isRequired,\n  cart: PropTypes.array.isRequired,\n  addItemToCart: PropTypes.func.isRequired,\n};\n\nfunction mapStateToProps(state, ownProps) {\n  return {\n    samsung: state.samsung,\n    cart: state.cart,\n  };\n}\n\nconst mapDispatchToProps = {\n  addItemToCart,\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(SamsungPage);\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/HuaweiPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/ApplePage.js",["237"],"import React from \"react\";\nimport PropTypes from \"prop-types\";\nimport { Row } from \"antd\";\nimport ListOfCardsDevices from \"../common/ListOfCardsDevices\";\nimport { addItemToCart } from \"../../redux/actions/cartActions\";\nimport { connect } from \"react-redux\";\nimport { withRouter } from \"react-router\";\n\nfunction ApplePage(props) {\n  function addBrandToObj(arrOfDevices) {\n    return { ...arrOfDevices, brand: \"apple\" };\n  }\n\n  const arrOfAllAppleGoods = Object.values(props.apple).flat();\n\n  function checkUrl() {\n    switch (props.location.pathname) {\n      case \"/apple\":\n        return arrOfAllAppleGoods;\n      case \"/apple/iphones\":\n        return props.apple.iphones;\n      case \"/apple/ipads\":\n        return props.apple.ipads;\n      case \"/apple/macbooks\":\n        return props.apple.macbooks;\n      default:\n        return undefined;\n    }\n  }\n  return (\n    <>\n      <h2>Apple</h2>\n      <Row>\n        <section className=\"list-devices-main-cont\">\n          <ListOfCardsDevices list={checkUrl()} />\n        </section>\n      </Row>\n    </>\n  );\n}\n\nApplePage.propTypes = {\n  apple: PropTypes.object.isRequired,\n  cart: PropTypes.array.isRequired,\n  addItemToCart: PropTypes.func.isRequired,\n};\n\nfunction mapStateToProps(state, ownProps) {\n  return {\n    apple: state.apple,\n    cart: state.cart,\n  };\n}\n\nconst mapDispatchToProps = {\n  addItemToCart,\n};\n\nexport default connect(\n  mapStateToProps,\n  mapDispatchToProps\n)(withRouter(ApplePage));\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/brands/XiaomiPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/cart/CartPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/cart/CheckoutPage.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/common/MenuForHeader.js",["238"],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/cart/PopUpCartWindow.js",["239"],"import React from \"react\";\nimport PropTypes from \"prop-types\";\nimport { DeleteOutlined } from \"@ant-design/icons\";\nimport { Button } from \"antd\";\nimport { NavLink } from \"react-router-dom\";\n\nconst PopUpCartWindow = (props) => (\n  <div className=\"pop-up-window\">\n    <ul className=\"pop-up-window-ul\">\n      <p className=\"pop-up-window-ul-title\">Recent add item(s)</p>\n      {props.cart.map((item) => (\n        <li key={item.id} className=\"pop-up-window-li\">\n          <NavLink to={\"/\" + item.brand + \"/\" + item.category + \"/\" + item.url}>\n            <img width=\"150px\" src={item.path}></img>\n          </NavLink>\n          <div>\n            <p>\n              {`${item.brand[0].toUpperCase() + item.brand.slice(1)} ${\n                item.title\n              } ${item.memory} ${item.color}`}\n            </p>\n            <p>\n              {item.quantity} x ${item.price}\n            </p>\n          </div>\n          <Button\n            className=\"pop-up-window-li-delete-btn\"\n            onClick={() => props.onDeleteItem(item)}\n          >\n            <DeleteOutlined />\n          </Button>\n        </li>\n      ))}\n    </ul>\n\n    <>\n      <h5 className=\"pop-up-window-subtotal\">\n        Subtotal: $\n        {props.cart.reduce((sum, current) => sum + parseInt(current.cost), 0)}\n      </h5>\n      <div className=\"pop-up-window-cont-viewcart-checkout\">\n        <NavLink to=\"/cart\">\n          <Button size=\"large\" type=\"primary\">\n            View Cart\n          </Button>\n        </NavLink>\n        <NavLink to=\"/checkout\">\n          <Button size=\"large\" type=\"primary\">\n            Checkout\n          </Button>\n        </NavLink>\n      </div>\n    </>\n  </div>\n);\n\nPopUpCartWindow.propTypes = {\n  cart: PropTypes.array.isRequired,\n  onDeleteItem: PropTypes.func.isRequired,\n};\n\nexport default PopUpCartWindow;\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/menu/SamsungMenu.js",["240","241"],"import React from \"react\";\r\nimport { Menu } from \"antd\";\r\nimport PropTypes from \"prop-types\";\r\nimport MenuCreator from \"./MenuCreator\";\r\n\r\nconst { SubMenu } = Menu;\r\n\r\nfunction SamsungMenu(props) {\r\n  return (\r\n    <Menu mode=\"vertical\" style={{ height: \"100%\" }}>\r\n      <SubMenu\r\n        key=\"galaxyS\"\r\n        title={<span>Galaxy S</span>}\r\n        onTitleClick={(obj) => console.log(obj)}\r\n      >\r\n        {props.galaxyS.map((item) => (\r\n          <Menu.Item key={item.id}>{item.title}</Menu.Item>\r\n        ))}\r\n      </SubMenu>\r\n      {/* <SubMenu key=\"ipad\" title={<span>iPad</span>}>\r\n        {props.ipads.map((item) => (\r\n          <Menu.Item key={item.id}>{item.title}</Menu.Item>\r\n        ))}\r\n      </SubMenu>\r\n      <SubMenu key=\"macBook\" title={<span>Mac Book</span>}>\r\n        {props.macbooks.map((item) => (\r\n          <Menu.Item key={item.id}>{item.title}</Menu.Item>\r\n        ))}\r\n      </SubMenu> */}\r\n    </Menu>\r\n  );\r\n}\r\n\r\nexport default SamsungMenu;\r\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/menu/AppleMenu.js",["242","243","244"],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/redux/actions/appleActions.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/redux/actions/cartActions.js",["245","246","247","248","249"],"import * as types from \"./actionsTypes\";\nimport {\n  getCart,\n  addItem,\n  deleteItem,\n  updateCartApi,\n  cleanCartApi,\n} from \"../../api/cartApi\";\n\nexport function loadCartSuccess(cart) {\n  return { type: types.LOAD_CART_SUCCESS, cart };\n}\n\nexport function addItemToCartSuccess(item) {\n  return { type: types.ADD_ITEM_TO_CART, item };\n}\n\nexport function deleteItemFromCartSuccess(item) {\n  return { type: types.DELETE_ITEM_FROM_CART, item };\n}\n\nexport function updateCartSuccess(updatedCart) {\n  return { type: types.UPDATE_CART_SUCCESS, updatedCart };\n}\n\nexport function cleanCartSuccess(currentCart) {\n  return { type: types.CLEAN_CART_SUCCESS, currentCart };\n}\n\nexport function addItemToCart(item) {\n  return function (dispatch) {\n    dispatch(addItemToCartSuccess(item));\n    // return addItem(item)\n    //   .then((addedItem) => dispatch(addItemToCartSuccess(addedItem)))\n    //   .catch((error) => console.error(error));\n  };\n}\n\n// export function loadCart() {\n//   return function (dispatch) {\n//     return getCart()\n//       .then((cart) => {\n//         dispatch(loadCartSuccess(cart));\n//       })\n//       .catch((error) => console.error(error));\n//   };\n// }\n\nexport function deleteItemFromCart(item) {\n  return (dispatch) => {\n    dispatch(deleteItemFromCartSuccess(item));\n    // return deleteItem(item);\n  };\n}\n\nexport function updateCart(updatedCart) {\n  return function (dispatch) {\n    dispatch(updateCartSuccess(updatedCart));\n    // return updatedCart.forEach((item) => updateCartApi(item));\n  };\n}\n\nexport function cleanCart(currentCart) {\n  return function (dispatch) {\n    dispatch(cleanCartSuccess(currentCart));\n    // return currentCart.forEach((item) => cleanCartApi(item));\n  };\n}\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/redux/actions/devicePageActions.js",[],"/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/cart/FormCheckout.js",["250"],"import React from \"react\";\r\nimport PropTypes from \"prop-types\";\r\nimport { Form, Input, Button, Select } from \"antd\";\r\nimport { statesOfAmerica } from \"../../USAstates\";\r\n\r\nconst validateMessages = {\r\n  required: \"${label} is required!\",\r\n};\r\n\r\nfunction FormCheckout(props) {\r\n  return (\r\n    <Form\r\n      className=\"checkout-form\"\r\n      name=\"order-form\"\r\n      onFinish={props.onFinish}\r\n      validateMessages={validateMessages}\r\n    >\r\n      <Form.Item\r\n        name={[\"first_name\"]}\r\n        label=\"First name\"\r\n        rules={[\r\n          {\r\n            required: true,\r\n          },\r\n        ]}\r\n      >\r\n        <Input />\r\n      </Form.Item>\r\n      <Form.Item\r\n        name={[\"second_name\"]}\r\n        label=\"Second name\"\r\n        rules={[\r\n          {\r\n            required: true,\r\n          },\r\n        ]}\r\n      >\r\n        <Input />\r\n      </Form.Item>\r\n      <Form.Item name={[\"company_name\"]} label=\"Company name (optional)\">\r\n        <Input />\r\n      </Form.Item>\r\n      <Form.Item name={[\"country\"]} label=\"Country\">\r\n        <p\r\n          className=\"checkout-form-title-usa\"\r\n          // style={{ margin: 0 }}\r\n        >\r\n          USA\r\n        </p>\r\n      </Form.Item>\r\n      <Form.Item\r\n        name={[\"state_of_country\"]}\r\n        label=\"State\"\r\n        rules={[\r\n          {\r\n            required: true,\r\n          },\r\n        ]}\r\n      >\r\n        <Select\r\n          defaultValue=\"Ohio\"\r\n          placeholder=\"Select state\"\r\n          onChange={(value) => props.setAmericanStateProp(value)}\r\n        >\r\n          {statesOfAmerica.map((state) => (\r\n            <Select.Option key={state} value={state}>\r\n              {state}\r\n            </Select.Option>\r\n          ))}\r\n        </Select>\r\n      </Form.Item>\r\n      <Form.Item\r\n        name={[\"street\"]}\r\n        label=\"Street address\"\r\n        rules={[{ required: true, message: \"Address is required\" }]}\r\n      >\r\n        <Input placeholder=\"House number and street name\" />\r\n      </Form.Item>\r\n\r\n      <Form.Item\r\n        name={[\"city\"]}\r\n        label=\"Town/City\"\r\n        rules={[{ required: true, message: \"Address is required\" }]}\r\n      >\r\n        <Input placeholder=\"Town/City name\" />\r\n      </Form.Item>\r\n\r\n      <Form.Item\r\n        name={[\"postal_code\"]}\r\n        label=\"Postal code\"\r\n        rules={[{ required: true, message: \"Postal code is required\" }]}\r\n      >\r\n        <Input />\r\n      </Form.Item>\r\n\r\n      <Form.Item\r\n        name=\"phone\"\r\n        label=\"Phone Number\"\r\n        rules={[\r\n          {\r\n            required: true,\r\n            message: \"Please input your phone number!\",\r\n          },\r\n        ]}\r\n      >\r\n        <Input addonBefore=\"+1\" />\r\n      </Form.Item>\r\n\r\n      <Form.Item\r\n        name=\"email\"\r\n        label=\"E-mail\"\r\n        rules={[\r\n          {\r\n            type: \"email\",\r\n            message: \"The input is not valid E-mail!\",\r\n          },\r\n          {\r\n            required: true,\r\n            message: \"Please input your E-mail!\",\r\n          },\r\n        ]}\r\n      >\r\n        <Input />\r\n      </Form.Item>\r\n      <Button\r\n        className=\"checkout-form-sbmt-btn\"\r\n        size=\"large\"\r\n        type=\"primary\"\r\n        // style={{ marginTop: \"20px\", width: \"100px\" }}\r\n        htmlType=\"submit\"\r\n      >\r\n        Submit\r\n      </Button>\r\n    </Form>\r\n  );\r\n}\r\n\r\nFormCheckout.propTypes = {\r\n  onFinish: PropTypes.func.isRequired,\r\n  setAmericanStateProp: PropTypes.func.isRequired,\r\n};\r\n\r\nexport default FormCheckout;\r\n","/home/roman/Documents/development/projects/second_the-worst-online-mobile-phone-store/src/components/cart/CardForCart.js",["251"],"import React from \"react\";\nimport PropTypes from \"prop-types\";\nimport Counter from \"./Counter\";\nimport { DeleteOutlined } from \"@ant-design/icons\";\nimport { Button } from \"antd\";\nimport { NavLink } from \"react-router-dom\";\n\nconst CardForCart = (props) => {\n  return (\n    <div className=\"card-cart\" key={props.item.id}>\n      <NavLink\n        className=\"card-cart-linkcont-img\"\n        to={`/${props.item.brand}/${props.item.category}/${props.item.url}`}\n      >\n        <img\n          // alt={props.irem.fullTitle}\n          className=\"card-cart-img\"\n          src={props.item.path}\n        ></img>\n      </NavLink>\n\n      <p className=\"card-cart-title\">{props.item.fullTitle}</p>\n      <div className=\"card-cart-cont-counterprice\">\n        <Counter\n          onChangeQuantity={(count) => props.onChangeQuantity(count)}\n          quantity={props.item.quantity}\n        ></Counter>\n\n        <p className=\"card-cart-price\">${props.item.cost}</p>\n      </div>\n\n      <Button\n        className=\"card-cart-deletebtn\"\n        id={props.item.id}\n        onClick={props.onDeleteItem}\n      >\n        <DeleteOutlined />\n      </Button>\n    </div>\n  );\n};\n\nCardForCart.propTypes = {\n  item: PropTypes.object.isRequired,\n  onChangeQuantity: PropTypes.func.isRequired,\n  onDeleteItem: PropTypes.func.isRequired,\n};\n\nexport default CardForCart;\n","/home/roman/Documents/development/pro